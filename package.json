{
	"name": "sourcegraph",
	"displayName": "source-graph",
	"description": "An interactive source graph  ",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other",
		"Visualization"
	],
	"main": "./out/src/extension.ts",
	"activationEvents": [
		"onCommand:sg.enable"
	],
	"contributes": {
		"viewsContainers": {
			"source-graph":[
				
			],
			"activity-bar": [
				{
					"id": "source-graph",
					"title": "source-graph",
					"icon": "resources/source-graph.svg"
				}
			]
			
		},
		"views":{
			"source-graph-scale":{
				
			}
		},
		"commands": [
			{
				"command": "sg.enable",
				"category": "source-graph",
				"title": "Surce graph explorer Enabled and set as default editor"
			},
			{
				"command": "sg.disable",
				"category": "source-graph",
				"title": "Source graph explorer disabled and Unset as default"
			},
			{
				"command": "sg.focus",
				"category": "source-graph",
				"title": "Focused on sidebar"
			}

		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"types/d3": "^7.1.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "^14.17.29",
		"@types/vscode": "^1.61.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"d3": "^7.1.1",
		"langium": "^0.1.0",
		"three": "^0.133.1",
		"tsc": "^2.0.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thatTPR/SourceGraph"
	},
	"bugs": {
		"url": "https://github.com/thatTPR/SourceGraph/issues"
	}
}