{
	"name": "interactive-sourcegraph",
	"displayName": "interactive-source-graph",
	"description": "An interactive source graph  ",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:interactive-source-graph.enable"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "interactive-source-graph.enable",
				"category": "interactive-source-graph",
				"title": "Hello World"
			},
			{
				"comand": "interactive-source-graph.3d",
				"category": "interactive-source-graph",
				"title" : "Enable 3d"
			}
		]
		
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/d3": "^7.0.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"d3": "^7.1.1",
		"tsc": "^2.0.3",
		"walker": "^1.0.7"
	}
}
