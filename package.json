{
	"name": "sourcegraph",
	"displayName": "source-graph",
	"description": "An interactive source graph  ",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other",
		"Visualization"
	],
	"main": "./src/extension.js",
	"activationEvents": [
		"onCommand:commandId"
	],
	"contributes": {
		"commands": [
			{
				"command": "source-graph.start",
				"category": "source-graph",
				"title": "Hello World"
			}
		],
		"viewsContainers": {
			"activity-bar": [
				{
					"id": "source-graph",
					"title": "source-graph",
					"icon": "resources/source-graph.svg"
				}
			]
		},
		"views": {
			
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/d3": "^7.1.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "^14.17.29",
		"@types/vscode": "^1.61.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"d3": "^7.1.1",
		"langium": "^0.1.0",
		"three": "^0.133.1",
		"tsc": "^2.0.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thatTPR/interactiveSourceGraph.git"
	  },
	  "bugs": {
		"url":"https://github.com/thatTPR/interactiveSourceGraph/issues"
	  }
}
